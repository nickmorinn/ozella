{%- if settings.color_footer == settings.color_body_bg -%}
  {%- style -%}
    .site-footer {
      border-top: 1px solid {{ settings.color_borders }};
    }
  {%- endstyle -%}
{%- endif -%}

{%- style -%}
  .site-footer[data-section-id="{{ section.id }}"] {
    margin-bottom: 0 !important;
    overflow: hidden;
    padding-bottom: 0 !important;
    padding-top: clamp(30px, 5.2vw, 64px);
    position: relative;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__small-text {
    margin-top: clamp(22px, 3vw, 34px);
    text-align: center;
  }

  .site-footer[data-section-id="{{ section.id }}"] .site-footer__linklist li {
    margin-bottom: 10px;
  }

  .site-footer[data-section-id="{{ section.id }}"] .site-footer__linklist a {
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback);
    font-size: max(calc(var(--typeBaseSize) - 4px), 12px);
    letter-spacing: 0.18em;
    text-transform: uppercase;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter {
    position: relative;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-input {
    background: rgba(255, 255, 255, 0.24);
    border: 1.5px solid rgba(0, 0, 0, 0.58);
    border-radius: 999px;
    min-height: 50px;
    padding: 13px 56px 11px 18px;
    transition: border-color 180ms ease, box-shadow 180ms ease, background-color 180ms ease;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-input:focus {
    background: rgba(255, 255, 255, 0.38);
    border: 1.5px solid rgba(0, 0, 0, 0.82);
    box-shadow: 0 0 0 3px rgba(0, 0, 0, 0.08);
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-input::placeholder {
    color: rgba(0, 0, 0, 0.56);
    font-family: var(--typeBodyPrimary), var(--typeBodyFallback);
    font-size: 0.98em;
    letter-spacing: 0.015em;
    text-transform: none;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-btn {
    align-items: center;
    background: rgba(0, 0, 0, 0.88);
    border: 0;
    border-radius: 999px;
    color: #fff;
    display: inline-flex;
    height: 32px;
    justify-content: center;
    right: 16px;
    width: 32px;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-btn .icon {
    height: 14px;
    width: 14px;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-btn-label,
  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-input--active + .footer__newsletter-btn .footer__newsletter-btn-label {
    display: none !important;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-input--active + .footer__newsletter-btn .icon {
    display: block !important;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer__newsletter-btn:hover {
    background: #000;
  }

  @media only screen and (min-width: 769px) {
    .site-footer[data-section-id="{{ section.id }}"] .grid {
      align-items: start;
      column-gap: clamp(22px, 2.4vw, 46px);
      display: grid;
      grid-template-columns: repeat(12, minmax(0, 1fr));
      row-gap: clamp(20px, 2.8vw, 36px);
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item {
      float: none;
      padding-top: 0 !important;
      width: auto !important;
    }

    .site-footer[data-section-id="{{ section.id }}"] .footer__clear {
      display: none;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="logo_social"] {
      grid-column: 1 / -1;
      justify-self: stretch;
      text-align: center;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="logo_social"] .footer__logo {
      height: auto;
      margin: clamp(28px, 3.6vw, 46px) 0 clamp(-170px, -10vw, -96px);
      max-width: 100%;
      overflow: visible;
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="logo_social"] .footer__logo a {
      display: block;
      height: auto !important;
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="logo_social"] .footer__logo image-element[data-aos="footer-logo-scroll-up"] {
      display: block;
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="logo_social"] .footer__logo .footer__logo-image {
      display: block;
      height: auto !important;
      max-height: none !important;
      max-width: 100% !important;
      width: 100% !important;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="menu"] {
      grid-column: 1 / span 5;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] {
      grid-column: 8 / -1;
      justify-self: stretch;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="custom"] {
      grid-column: 8 / -1;
      justify-self: stretch;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__item-padding {
      padding-right: 0;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__collapsible {
      margin-left: auto;
      max-width: min(100%, 500px);
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__title,
    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="menu"] .footer__title {
      margin-bottom: clamp(14px, 1.8vw, 24px);
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__newsletter {
      display: block;
      max-width: none;
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__newsletter-input {
      max-width: none;
      padding-right: 54px;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__social {
      display: flex;
      justify-content: flex-start;
      margin-top: clamp(10px, 1vw, 16px);
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__social li {
      margin: 0;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__social a {
      align-items: center;
      border: 0;
      border-radius: 0;
      display: inline-flex;
      height: auto;
      justify-content: center;
      padding: 0;
      transition: transform 180ms ease, opacity 180ms ease;
      width: auto;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__social a:hover {
      transform: translateY(-1px);
      opacity: 0.72;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__social .icon {
      height: 22px;
      width: 22px;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__social .icon path {
      fill: currentColor !important;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] p {
      margin-bottom: clamp(12px, 1.6vw, 18px);
      max-width: 40ch;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="menu"] .site-footer__linklist {
      column-gap: clamp(34px, 4.2vw, 72px);
      display: grid;
      grid-template-columns: repeat(2, minmax(0, 1fr));
      row-gap: 2px;
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="menu"] .site-footer__linklist li {
      margin-bottom: 0;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="menu"] .site-footer__linklist a {
      width: max-content;
    }
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer-floaters {
    --footer-float-progress: 0;
    bottom: 0;
    left: 0;
    overflow: visible;
    pointer-events: none;
    position: absolute;
    right: 0;
    top: clamp(220px, 21vw, 300px);
    z-index: 1;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer-floater {
    --drift: 0px;
    --rise: 92px;
    --rotation: 0deg;
    --scale: 1;
    bottom: clamp(220px, 18vw, 300px);
    left: 50%;
    position: absolute;
    transform: translate3d(
      calc(var(--drift) * var(--footer-float-progress)),
      calc((1 - var(--footer-float-progress)) * 76px - (var(--footer-float-progress) * var(--rise))),
      0
    ) rotate(var(--rotation)) scale(var(--scale));
    transition: transform 220ms cubic-bezier(0.22, 1, 0.36, 1);
    transform-origin: 50% 100%;
    will-change: transform;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer-floater img {
    display: block;
    height: auto;
    width: 100%;
  }

  .site-footer[data-section-id="{{ section.id }}"] .footer-floater--2 {
    --drift: -3px;
    --rise: 46px;
    --rotation: 0deg;
    --scale: 0.72;
    bottom: clamp(242px, 18vw, 292px);
    left: 87.5%;
    width: clamp(46px, 4vw, 68px);
  }

  @media only screen and (max-width: 768px) {
    .site-footer[data-section-id="{{ section.id }}"] {
      padding-top: clamp(24px, 7vw, 40px);
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid {
      display: grid;
      gap: clamp(16px, 4.6vw, 26px);
      grid-template-columns: minmax(0, 1fr);
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item {
      float: none;
      padding-top: 0 !important;
      width: 100% !important;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item:after {
      display: none;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] {
      order: 1;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="menu"] {
      order: 2;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="custom"] {
      order: 3;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="logo_social"] {
      order: 4;
    }

    .site-footer[data-section-id="{{ section.id }}"] .page-width {
      z-index: auto;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="menu"],
    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"],
    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="custom"] {
      position: relative;
      z-index: 3;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="logo_social"] {
      position: relative;
      z-index: 1;
    }

    .site-footer[data-section-id="{{ section.id }}"] .footer__item-padding {
      padding-right: 0;
    }

    .site-footer[data-section-id="{{ section.id }}"] .footer__collapsible {
      padding-bottom: 0;
    }

    .site-footer[data-section-id="{{ section.id }}"] .footer_collapsible--disabled {
      padding-top: 0;
    }

    .site-footer[data-section-id="{{ section.id }}"] .site-footer__linklist {
      column-gap: clamp(20px, 6vw, 32px);
      display: grid;
      grid-template-columns: repeat(2, minmax(0, 1fr));
      row-gap: 6px;
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .site-footer__linklist li {
      margin-bottom: 0;
    }

    .site-footer[data-section-id="{{ section.id }}"] .site-footer__linklist a {
      width: max-content;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="logo_social"] .footer__logo {
      margin: 10px 0 clamp(-18px, -5vw, -6px);
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__newsletter {
      display: block;
      max-width: none;
      width: 100%;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__newsletter-input {
      max-width: none;
      min-height: 48px;
      padding: 12px 52px 10px 16px;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__social {
      display: flex;
      justify-content: flex-end;
      margin-top: 10px;
    }

    .site-footer[data-section-id="{{ section.id }}"] .grid > .grid__item[data-type="newsletter"] .footer__social li {
      margin: 0;
    }

    .site-footer[data-section-id="{{ section.id }}"] .footer-floaters {
      overflow: visible;
      top: clamp(372px, 88vw, 548px);
      z-index: 2;
    }

    .site-footer[data-section-id="{{ section.id }}"] .footer-floater--2 {
      --drift: -2px;
      --rise: 24px;
      bottom: clamp(148px, 26vw, 198px);
      left: 88.5%;
      width: clamp(26px, 7.6vw, 38px);
    }
  }

  @media (prefers-reduced-motion: reduce) {
    .site-footer[data-section-id="{{ section.id }}"] .footer-floaters {
      display: none;
    }
  }

  [data-disable-animations=true] .site-footer[data-section-id="{{ section.id }}"] .footer-floaters {
    display: none;
  }

  @media only screen and (max-width: 768px) {
    [data-disable-animations=true] .site-footer[data-section-id="{{ section.id }}"] .footer-floaters {
      --footer-float-progress: 1;
      display: block;
    }
  }

  @media only screen and (max-width: 768px) and (prefers-reduced-motion: reduce) {
    .site-footer[data-section-id="{{ section.id }}"] .footer-floaters {
      --footer-float-progress: 1;
      display: block;
    }
  }

  .site-footer[data-section-id="{{ section.id }}"] .page-width {
    position: relative;
    z-index: 2;
  }

  .site-footer[data-section-id="{{ section.id }}"] .page-width {
    padding-bottom: 0 !important;
  }
{%- endstyle -%}

<footer class="site-footer" data-section-id="{{ section.id }}" data-section-type="footer-section">
  <div class="page-width">

    <div class="grid">
      {%- assign row_width = 0 -%}
      {%- for block in section.blocks -%}
        {%- assign row_width = row_width | plus: block.settings.container_width -%}
        {%- if row_width > 100 -%}
          <div class="footer__clear small--hide"></div>
          {%- assign row_width = row_width | minus: 100 -%}
        {%- endif -%}

        <div {{ block.shopify_attributes }} class="grid__item footer__item--{{ block.id }}" data-type="{{ block.type }}">
          {%- style -%}
            @media only screen and (min-width: 769px) and (max-width: 959px) {
              .footer__item--{{ block.id }} {
                width: 50%;
                padding-top: 40px;
              }
              .footer__item--{{ block.id }}:nth-child(2n + 1) {
                clear: left;
              }
            }
            @media only screen and (min-width: 960px) {
              .footer__item--{{ block.id }} {
                width: {{ block.settings.container_width }}%;
              }

            }
          {%- endstyle -%}

          {%- liquid
            case block.type
              when 'logo_social'
                render 'footer-logo', block: block
              when 'custom'
                render 'footer-custom-text', block: block, container_width: block.settings.container_width
              when 'newsletter'
                render 'footer-newsletter', block: block
              when 'menu'
                render 'footer-menu', block: block
              when 'follow_shop_cta'
                render 'follow-shop-cta', block: block
            endcase
          -%}
        </div>
      {%- endfor -%}
    </div>

    {%- if section.settings.show_payment_icons -%}
      {%- unless shop.enabled_payment_types == empty -%}
        <ul class="inline-list payment-icons footer__section">
          {%- for type in shop.enabled_payment_types -%}
            <li class="icon--payment">
              {{ type | payment_type_svg_tag }}
            </li>
          {%- endfor -%}
        </ul>
      {%- endunless -%}
    {%- endif -%}

    {%- if section.settings.show_copyright -%}
      <p class="footer__small-text">
        &copy; {{ 'now' | date: '%Y' }} {{ shop.name }}
        {%- if section.settings.copyright_text != blank -%}
          {{ section.settings.copyright_text }}
        {%- endif -%}
      </p>
    {%- endif -%}

  </div>

  <div class="footer-floaters" data-footer-floaters="{{ section.id }}" aria-hidden="true">
    <span class="footer-floater footer-floater--2">
      <img src="{{ 'hand-cropped.svg' | asset_url }}" alt="" loading="lazy" width="120" height="120">
    </span>
  </div>
</footer>

<script>
  (function() {
    var sectionId = '{{ section.id }}';

    var initFooterFloaters = function() {
      var footer = document.querySelector('.site-footer[data-section-id="' + sectionId + '"]');
      var floaters = document.querySelector('.site-footer[data-section-id="' + sectionId + '"] [data-footer-floaters="' + sectionId + '"]');
      if (!footer || !floaters || floaters.dataset.bound === 'true') return;

      floaters.dataset.bound = 'true';

      if (document.body && document.body.dataset.disableAnimations === 'true') return;
      if (window.matchMedia && window.matchMedia('(prefers-reduced-motion: reduce)').matches) return;

      var rafId = 0;

      var update = function() {
        rafId = 0;
        var rect = footer.getBoundingClientRect();
        var start = window.innerHeight * 1.7;
        var end = window.innerHeight * -0.12;
        var progress = (start - rect.top) / (start - end);
        progress = Math.max(0, Math.min(1, progress));

        floaters.style.setProperty('--footer-float-progress', progress.toFixed(4));
      };

      var requestUpdate = function() {
        if (rafId) return;
        rafId = window.requestAnimationFrame(update);
      };

      requestUpdate();
      window.addEventListener('scroll', requestUpdate, { passive: true });
      window.addEventListener('resize', requestUpdate);
      document.addEventListener('shopify:section:load', requestUpdate);
    };

    if (document.readyState === 'loading') {
      document.addEventListener('DOMContentLoaded', initFooterFloaters, { once: true });
    } else {
      initFooterFloaters();
    }
  })();
</script>

