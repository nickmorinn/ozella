{%- style -%}
    :root {
    --typeHeaderPrimary: "Contempora Sans";
    --typeHeaderFallback: {{ settings.type_header_font_family.family }}, {{ settings.type_header_font_family.fallback_families }};
    --typeHeaderSize: {{ settings.type_header_base_size | default: '32' | append: 'px' }};
    --typeHeaderWeight: {{ settings.type_header_font_family.weight }};
    --typeHeaderLineHeight: {{ settings.type_header_line_height | default: 1.4 }};
    --typeHeaderSpacing: {{ settings.type_header_spacing | default: '25' | divided_by: 1000.00 | append: 'em' }};

    --typeBasePrimary: "Freight Text Pro";
    --typeBaseFallback: {{ settings.type_base_font_family.family }}, {{ settings.type_base_font_family.fallback_families }};
    --typeBaseSize: {{ settings.type_base_size | default: 16 | append: 'px' }};
    --typeBaseWeight: {{ settings.type_base_font_family.weight }};
    --typeBaseSpacing: {{ settings.type_base_spacing | default: '50' | divided_by: 1000.00 | append: 'em' }};
    --typeBaseLineHeight: {{ settings.type_base_line_height | default: 1.4 }};
    --typeBaselineHeightMinus01: {{ settings.type_base_line_height | default: 1.4 | minus: 0.1 }};

    --typeCollectionTitle: {{ settings.type_collection_size | append: 'px' }};

    --typeAccentPrimary: "GeneralGrotesqueMono-Regular";
    --typeAccentFallback: {{ settings.type_header_font_family.family }}, {{ settings.type_header_font_family.fallback_families }};

    --element-button-font-family: var(--typeAccentPrimary);
    --element-button-font-family-fallback: var(--typeAccentFallback);
    --element-button-font-weight: 400;
    --element-button-text-transform: uppercase;

    /* Typographic consistency scale */
    --oz-type-ui-xs: 12px;
    --oz-type-ui-sm: 13px;
    --oz-type-copy: max(calc(var(--typeBaseSize) - 2px), 14px);
    --oz-type-copy-compact: max(calc(var(--typeBaseSize) - 3px), 13px);
    --oz-type-display-sm: clamp(28px, 4vw, 40px);
    --oz-track-ui: 0.06em;
    --oz-space-1: 8px;
    --oz-space-2: 12px;
    --oz-space-3: 16px;
    --oz-space-4: 24px;

    --iconWeight: {{ settings.icon_weight | default: '5px' }};
    --iconLinecaps: {{ settings.icon_linecaps | default: 'miter' }};

    {% if settings.button_style == 'round-slight' %}
        --buttonRadius: 3px;
    {% elsif settings.button_style == 'round' %}
        --buttonRadius: 50px;
    {% else %}
        --buttonRadius: 0;
    {% endif %}

    --colorGridOverlayOpacity: {{ settings.collection_grid_opacity | divided_by: 100.0 }};
    --colorAnnouncement: {{ settings.color_announcement | default: "#1c1d1d" }};
    --colorAnnouncementText: {{ settings.color_announcement_text | default: "#fff" }};

    --colorBody: {{ settings.color_body_bg | default: "#fff" }};
    --colorBodyAlpha05: {{ settings.color_body_bg | default: "#fff" | color_modify: 'alpha', 0.05 }};
    --colorBodyDim: {{ settings.color_body_bg | default: "#1c1d1d" | color_darken: 5 }};
    --colorBodyLightDim: {{ settings.color_body_bg | default: '#fff' | color_darken: 2 }};
    --colorBodyMediumDim: {{ settings.color_body_bg | default: '#fff' | color_darken: 4 }};


    --colorBorder: {{ settings.color_borders | default: "#1c1d1d" }};

    --colorBtnPrimary: {{ settings.color_button | default: "#000" }};
    --colorBtnPrimaryLight: {{ settings.color_button | default: "#000" | color_lighten: 10 }};
    --colorBtnPrimaryDim: {{ settings.color_button | default: "#000" | color_darken: 5 }};
    --colorBtnPrimaryText: {{ settings.color_button_text | default: "#fff" }};

    --colorCartDot: {{ settings.color_cart_dot | default: "#ff4f33" }};

    --colorDrawers: {{ settings.color_drawer_background | default: "#1c1d1d" }};
    --colorDrawersDim: {{ settings.color_drawer_background | default: "#1c1d1d" | color_darken: 5 }};
    --colorDrawerBorder: {{ settings.color_drawer_border | default: "#343535" }};
    --colorDrawerText: {{ settings.color_drawer_text | default: "#fff" }};
    --colorDrawerTextDark: {{ settings.color_drawer_text | default: "#fff" | color_darken: 15 }};
    --colorDrawerButton: {{ settings.color_drawer_button | default: "#a26b25" }};
    --colorDrawerButtonText: {{ settings.color_drawer_button_text | default: "#fff" }};

    --colorFooter: {{ settings.color_footer | default: "#111" }};
    --colorFooterText: {{ settings.color_footer_text | default: "#fff" }};
    --colorFooterTextAlpha01: {{ settings.color_footer_text | default: "#fff" }};

    --colorGridOverlay: {{ settings.collection_grid_tint | default: "#000" }};
    --colorGridOverlayOpacity: 0.1;

    --colorHeaderTextAlpha01: {{ settings.color_header_text | default: '#000' | color_modify: 'alpha', 0.1 }};

    --colorHeroText: {{ settings.color_image_text | default: "#fff" }};

    --colorSmallImageBg: {{ settings.color_small_image_bg | default: "#eee" }};
    --colorLargeImageBg: {{ settings.color_large_image_bg | default: "#1c1d1d" }};

    --colorImageOverlay: {{ settings.color_image_overlay | default: "#000" }};
    --colorImageOverlayOpacity: {{ settings.color_image_overlay_opacity | divided_by: 100.0 }};
    --colorImageOverlayTextShadow: {{ settings.color_image_overlay_text_shadow | divided_by: 100.0 }};

    --colorLink: {{ settings.color_body_text | default: "#1c1d1d" }};

    --colorModalBg: {{ settings.color_modal_overlays | default: "#000" | color_modify: 'alpha', 0.6 }};

    --colorNav: {{ settings.color_header | default: "#fff" }};
    --colorNavText: {{ settings.color_header_text | default: "#000" }};

    --colorPrice: {{ settings.color_price | default: "#1c1d1d" }};

    --colorSaleTag: {{ settings.color_sale_tag | default: "#1c1d1d" }};
    --colorSaleTagText: {{ settings.color_sale_tag_text | default: "#ffffff" }};

    --colorTextBody: {{ settings.color_body_text | default: "#1c1d1d" }};
    --colorTextBodyAlpha015: {{ settings.color_body_text | default: '#1c1d1d' | color_modify: 'alpha', 0.15 }};
    --colorTextBodyAlpha005: {{ settings.color_body_text | default: "#1c1d1d"  | color_modify: 'alpha', 0.05 }};
    --colorTextBodyAlpha008: {{ settings.color_body_text | default: '#1c1d1d' | color_modify: 'alpha', 0.08 }};
    --colorTextSavings: {{ settings.color_savings_text | default: "#1c1d1d" }};

    --urlIcoSelect: url({{ 'ico-select.svg' | asset_url | split: '?' | first }});
    --urlIcoSelectFooter: url({{ 'ico-select-footer.svg' | asset_url | split: '?' | first }});
    --urlIcoSelectWhite: url({{ 'ico-select-white.svg' | asset_url | split: '?' | first }});

    --grid-gutter: 17px;
    --drawer-gutter: 20px;

    --sizeChartMargin: 25px 0;
    --sizeChartIconMargin: 5px;

    --newsletterReminderPadding: 40px;

    /*Shop Pay Installments*/
    --color-body-text: {{ settings.color_body_text | default: "#1c1d1d" }};
    --color-body: {{ settings.color_body_bg | default: "#fff" }};
    --color-bg: {{ settings.color_body_bg | default: "#fff" }};
    }

    .btn,
    .rte .btn,
    .shopify-payment-button .shopify-payment-button__button--unbranded,
    .collapsible-trigger-btn {
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback);
    letter-spacing: normal;
    line-height: 1.05;
    text-transform: uppercase;
    }

    .btn:not(.add-to-cart),
    .rte .btn:not(.add-to-cart),
    .shopify-payment-button .shopify-payment-button__button--unbranded {
    padding-bottom: 11px;
    padding-top: 11px;
    }

    .btn--small:not(.add-to-cart),
    .rte .btn--small:not(.add-to-cart),
    .shopify-payment-button .shopify-payment-button__button--unbranded.btn--small {
    padding-bottom: 8px;
    padding-top: 8px;
    }

    .btn--large:not(.add-to-cart),
    .rte .btn--large:not(.add-to-cart),
    .shopify-payment-button .shopify-payment-button__button--unbranded.btn--large {
    padding-bottom: 15px;
    padding-top: 15px;
    }

    .btn--full:not(.add-to-cart),
    .rte .btn--full:not(.add-to-cart),
    .shopify-payment-button .shopify-payment-button__button--unbranded.btn--full {
    padding-bottom: 13px;
    padding-top: 13px;
    }

    @media only screen and (max-width: 768px) {
    .btn:not(.add-to-cart),
    .rte .btn:not(.add-to-cart),
    .shopify-payment-button .shopify-payment-button__button--unbranded {
    padding-bottom: 9px;
    padding-top: 9px;
    }
    }

    .is-safari .btn,
    .is-safari .rte .btn,
    .is-safari .shopify-payment-button .shopify-payment-button__button--unbranded {
    align-items: center;
    display: inline-flex;
    justify-content: center;
    line-height: 1.12;
    }

    .is-safari .btn.btn--full,
    .is-safari .add-to-cart,
    .is-safari #CartDrawer .cart__checkout {
    display: flex;
    }

    .is-safari .collapsible-trigger-btn {
    align-items: center;
    display: flex;
    justify-content: flex-start;
    line-height: 1.12;
    }

    [data-center-text=true] .is-safari .collapsible-trigger-btn {
    justify-content: center;
    }

    .variant__label[for],
    .variant__label-info {
    letter-spacing: 0 !important;
    }

    .template-collection .collection-filter .btn,
    .template-collection .collection-filter__sort-container,
    .template-collection .collection-filter__sort-container select,
    .template-collection .collection-filter__sort-container #SortBy,
    .template-collection .collection-filter__sort-container option {
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback) !important;
    font-size: 13px;
    letter-spacing: 0.06em;
    line-height: 1;
    text-transform: uppercase;
    }

    .template-collection .collection-filter .btn {
    align-items: center;
    display: flex;
    gap: 8px;
    justify-content: flex-start;
    padding-left: 14px;
    padding-right: 14px;
    text-align: left;
    width: 100%;
    }

    .template-collection .collection-filter .btn .icon {
    margin-right: 0;
    top: 0;
    }

    .template-collection .collection-filter__sort-container select,
    .template-collection .collection-filter__sort-container #SortBy {
    background-color: var(--colorBody);
    border: 1px solid var(--colorBorder);
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback) !important;
    font-size: 13px !important;
    font-weight: 400 !important;
    letter-spacing: 0.06em !important;
    line-height: 1;
    padding-left: 14px;
    padding-right: 34px;
    text-align-last: left;
    text-align: left;
    text-transform: uppercase !important;
    }

    .template-collection .collection-filter .btn,
    .template-collection .collection-filter__sort-container select,
    .template-collection .collection-filter__sort-container #SortBy {
    height: 42px;
    min-height: 42px;
    }

    .template-collection .grid-product__title,
    .template-collection .grid-product__title--heading,
    .template-collection .grid-product__price,
    [data-context='featured-collection'] .grid-product__title,
    [data-context='featured-collection'] .grid-product__title--heading,
    [data-context='featured-collection'] .grid-product__price {
    font-size: calc(var(--typeBaseSize) * 0.85) !important;
    line-height: 1.2;
    }

    .grid-product__price::before {
    content: "[ ";
    opacity: 0.5;
    }

    .grid-product__price::after {
    content: " ]";
    opacity: 0.5;
    }

    @media only screen and (min-width: 769px) {
    .template-index .featured-collections__page-width {
    max-width: none;
    padding-left: 20px;
    padding-right: 20px;
    }

    .template-index .featured-collections__grid {
    margin-left: -6px;
    }

    .template-index .featured-collections__grid > .grid__item {
    padding-left: 6px;
    }

    .template-index .featured-collections__grid .collection-item {
    margin-bottom: 8px;
    }

    .template-collection .collection-content .page-width {
    max-width: none;
    padding-left: 24px;
    padding-right: 24px;
    }

    .template-collection .collection-content .grid__item--sidebar {
    width: 14.5%;
    }

    .template-collection .collection-content .grid__item--content {
    width: 85.5%;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .collection-sidebar {
    margin-bottom: 12px;
    padding-right: 6px;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .collection-sidebar__group {
    margin-bottom: 2px;
    margin-top: 2px;
    padding-top: 2px;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .collection-sidebar__group .collapsible-trigger-btn,
    .template-collection #CollectionSidebar[data-style='sidebar'] .tag-list__header {
    font-size: 12px;
    letter-spacing: 0.16em;
    padding-bottom: 9px;
    padding-top: 9px;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .tag-list li {
    margin-bottom: 4px;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .tag-list label,
    .template-collection #CollectionSidebar[data-style='sidebar'] .tag-list a {
    font-size: 13px;
    line-height: 1.25;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .tag__checkbox {
    padding-left: 20px;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .tag__checkbox:before {
    height: 13px;
    width: 13px;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .tag__checkbox:after {
    height: 7px;
    left: 3px;
    width: 7px;
    }

    .template-collection #CollectionSidebar[data-style='sidebar'] .color-swatch--filter {
    height: 28px;
    width: 28px;
    }
    }

    .template-collection .collection-load-more {
    display: flex;
    justify-content: center;
    margin: 10px 0 30px;
    }

    .template-collection [data-collection-load-more].is-loading {
    opacity: 0.6;
    pointer-events: none;
    }

    .template-policy .main-content {
    background: var(--colorBody);
    }

    .template-policy .shopify-policy__container {
    background: var(--colorBody);
    border: 1px solid var(--colorBorder);
    box-shadow: 0 20px 46px rgba(0, 0, 0, 0.08);
    margin: 22px auto 52px;
    max-width: min(980px, calc(100% - 34px));
    overflow: hidden;
    padding: 32px 22px 28px;
    position: relative;
    }

    .template-policy .shopify-policy__container::before {
    background: linear-gradient(90deg, var(--colorTextBody) 0, var(--colorTextBody) 72px, transparent 72px, transparent 100%);
    content: '';
    display: block;
    height: 3px;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    }

    .template-policy .shopify-policy__title {
    border-bottom: 1px solid var(--colorBorder);
    margin: 0 0 24px;
    padding: 0 0 16px;
    position: relative;
    text-align: left;
    }

    .template-policy .shopify-policy__title::before {
    color: var(--colorTextBodyAlpha06, rgba(0, 0, 0, 0.64));
    content: 'Store Policy';
    display: block;
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback);
    font-size: 11px;
    letter-spacing: 0.2em;
    line-height: 1;
    margin: 0 0 13px;
    text-transform: uppercase;
    }

    .template-policy .shopify-policy__title h1,
    .template-policy .shopify-policy__container > h1 {
    font-family: var(--typeHeaderPrimary), var(--typeHeaderFallback);
    font-size: clamp(30px, 6vw, 54px);
    letter-spacing: 0.01em;
    line-height: 0.94;
    margin: 0;
    text-transform: none;
    }

    .template-policy .shopify-policy__body {
    max-width: 72ch;
    }

    .template-policy .shopify-policy__body .rte {
    font-size: calc(var(--typeBaseSize) * 1.01);
    line-height: 1.64;
    }

    .template-policy .shopify-policy__body .rte > *:last-child {
    margin-bottom: 0;
    }

    .template-policy .shopify-policy__body .rte h2 {
    border-top: 0;
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback);
    font-size: 12px;
    letter-spacing: 0.16em;
    line-height: 1;
    margin: 40px 0 14px;
    padding: 0;
    text-transform: uppercase;
    }

    .template-policy .shopify-policy__body .rte h2:first-of-type {
    margin-top: 8px;
    }

    .template-policy .shopify-policy__body .rte h2 + p,
    .template-policy .shopify-policy__body .rte h2 + ul,
    .template-policy .shopify-policy__body .rte h2 + ol {
    margin-top: 0;
    }

    .template-policy .shopify-policy__body .rte h3,
    .template-policy .shopify-policy__body .rte h4 {
    font-family: var(--typeBasePrimary), var(--typeBaseFallback);
    font-size: calc(var(--typeBaseSize) * 1.18);
    font-weight: var(--typeBaseWeight);
    letter-spacing: 0.01em;
    line-height: 1.3;
    margin: 22px 0 10px;
    text-transform: none;
    }

    .template-policy .shopify-policy__body .rte p,
    .template-policy .shopify-policy__body .rte li {
    line-height: 1.66;
    }

    .template-policy .shopify-policy__body .rte ul,
    .template-policy .shopify-policy__body .rte ol {
    margin: 10px 0 18px;
    padding-left: 1.2em;
    }

    .template-policy .shopify-policy__body .rte li + li {
    margin-top: 6px;
    }

    .template-policy .shopify-policy__body .rte a {
    border-bottom: 1px solid currentColor;
    color: var(--colorTextBody);
    text-decoration: none;
    text-underline-offset: 0.12em;
    transition: color 220ms ease, border-color 220ms ease;
    }

    .template-policy .shopify-policy__body .rte a:hover {
    border-color: transparent;
    color: var(--colorTextBodyAlpha06, rgba(0, 0, 0, 0.64));
    }

    .template-policy .shopify-policy__body .rte table {
    border: 1px solid var(--colorBorder);
    border-collapse: collapse;
    margin: 18px 0 24px;
    width: 100%;
    }

    .template-policy .shopify-policy__body .rte td,
    .template-policy .shopify-policy__body .rte th {
    border: 1px solid var(--colorBorder);
    padding: 10px 12px;
    vertical-align: top;
    }

    .template-policy .shopify-policy__body .rte blockquote {
    border-left: 2px solid var(--colorTextBody);
    margin: 16px 0 20px;
    padding: 8px 0 8px 16px;
    }

    @media only screen and (min-width: 769px) {
    .template-policy .shopify-policy__container {
    margin: 44px auto 82px;
    padding: 48px 56px 42px;
    }
    }

    @media only screen and (max-width: 768px) {
    .template-policy .shopify-policy__container {
    max-width: calc(100% - 24px);
    }

    .template-policy .shopify-policy__body .rte {
    font-size: calc(var(--typeBaseSize) * 0.97);
    }
    }

    @media (prefers-reduced-motion: no-preference) {
    .template-policy .shopify-policy__container {
    animation: oz-policy-shell-in 420ms cubic-bezier(0.22, 1, 0.36, 1) both;
    }

    .template-policy .shopify-policy__title h1,
    .template-policy .shopify-policy__container > h1 {
    animation: oz-policy-title-in 480ms cubic-bezier(0.16, 1, 0.3, 1) 70ms both;
    }
    }

    @keyframes oz-policy-shell-in {
    from {
    opacity: 0;
    transform: translateY(14px);
    }

    to {
    opacity: 1;
    transform: translateY(0);
    }
    }

    @keyframes oz-policy-title-in {
    from {
    opacity: 0;
    transform: translateY(8px);
    }

    to {
    opacity: 1;
    transform: translateY(0);
    }
    }

    .template-page .page-content--about-ozella {
    background: #f2f2f2;
    border: 1px solid rgba(0, 0, 0, 0.12);
    border-radius: 12px;
    box-shadow: none;
    margin: clamp(20px, 3.2vw, 42px) auto clamp(40px, 5vw, 76px);
    max-width: min(1220px, calc(100% - 36px));
    overflow: hidden;
    padding: clamp(30px, 4vw, 52px) clamp(22px, 4.5vw, 56px) clamp(38px, 5vw, 62px);
    position: relative;
    }

    .template-page .page-content--about-ozella::before {
    content: none;
    }

    .template-page .page-content--about-ozella > * {
    position: relative;
    z-index: auto;
    }

    .template-page .page-content--about-ozella .about-ozella__orb {
    display: none;
    }

    .template-page .page-content--about-ozella .about-ozella__orb--one {
    display: none;
    }

    .template-page .page-content--about-ozella .about-ozella__orb--two {
    display: none;
    }

    .template-page .page-content--about-ozella .breadcrumb {
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback);
    font-size: 11px;
    letter-spacing: 0.12em;
    margin-bottom: clamp(20px, 2.6vw, 32px);
    opacity: 0.7;
    text-transform: uppercase;
    }

    .template-page .page-content--about-ozella .breadcrumb a {
    color: var(--colorTextBody);
    opacity: 0.68;
    text-decoration: none;
    transition: opacity 180ms ease;
    }

    .template-page .page-content--about-ozella .breadcrumb a:hover {
    opacity: 1;
    }

    .template-page .page-content--about-ozella .about-ozella__header {
    margin: 0 0 clamp(22px, 3vw, 34px);
    width: min(100%, 43ch);
    }

    .template-page .page-content--about-ozella .about-ozella__eyebrow {
    align-items: center;
    display: inline-flex;
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback);
    font-size: clamp(11px, 1vw, 12px);
    letter-spacing: 0.18em;
    line-height: 1;
    margin: 0 0 16px;
    text-transform: uppercase;
    }

    .template-page .page-content--about-ozella .about-ozella__eyebrow::after {
    background: rgba(0, 0, 0, 0.42);
    border-radius: 999px;
    content: '';
    display: inline-block;
    height: 7px;
    margin-left: 10px;
    width: 7px;
    }

    .template-page .page-content--about-ozella .section-header__title {
    font-family: var(--typeAccentPrimary), var(--typeAccentFallback);
    font-size: clamp(30px, 4.8vw, 46px);
    letter-spacing: 0.06em;
    line-height: 0.9;
    margin: 0 !important;
    max-width: 12ch;
    position: relative;
    text-transform: uppercase;
    }

    .template-page .page-content--about-ozella .section-header__title::after {
    background: rgba(0, 0, 0, 0.34);
    border-radius: 999px;
    content: '';
    display: block;
    height: 2px;
    margin-top: 14px;
    width: clamp(84px, 18vw, 150px);
    }

    .template-page .page-content--about-ozella .about-ozella__story {
    color: rgba(0, 0, 0, 0.9);
    line-height: 1.55;
    max-width: 100%;
    }

